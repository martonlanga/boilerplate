{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": ["error", "never"],
    "quotes": [2, "single", "avoid-escape"],
    "object-curly-spacing": ["error", "always"],
    "react/prop-types": 0,
    "react/jsx-uses-vars": [2],
    "no-console": 0
  },
  "parser": "babel-eslint"
}
